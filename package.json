{
  "name": "invillia",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": " run-s  build:dev start:server",
    "start:server": "webpack-dev-server --mode development --open",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "clean": "rm -rf build",
    "commit": "git-cz",
    "lint": "eslint ./src --fix",
    "prettier": "prettier --write \"src/**\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test": "jest --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dansantanna/invillia.git"
  },
  "keywords": [
    "react",
    "javascript",
    "styled-components"
  ],
  "author": "Daniel Sant'Anna <danielcarlossantanna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dansantanna/invillia/issues"
  },
  "homepage": "https://github.com/dansantanna/invillia#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.1",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-watch": "^6.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "sass-loader": "^7.2.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "design-system-utils": "^1.5.0",
    "nuka-carousel": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-content-loader": "^4.3.3",
    "react-dom": "^16.9.0",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "source-map": "^0.7.3",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "modulePaths": [
      "__mocks__"
    ],
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "setupFiles": ["<rootDir>/setupTests.js"]
  }
}
